{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AssumeCentralRole",
            "Effect": "Allow",
            "Action": "sts:AssumeRole",
            "Resource": "arn:aws:iam::665096241598:role/CentralAccountAccessRole"
        },
        {
            "Effect" : "Allow",
            "Action" : [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource" : "*"
        }
    ]
}